<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wcccd.wcccditcareerprograms.CisComputerSupportSpecialist">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="@dimen/xlargeLand_Img_width"
        android:layout_height="@dimen/xlargeLand_Img_height"
        app:srcCompat="@drawable/logo_wcccd"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="@dimen/xlargeLandTextOverView_width"
        android:layout_height="293dp"
        android:gravity="left"
        android:text="@string/ComputerSupportSpecialist_Overview"
        android:textSize="@dimen/xlargeLandTextOverView_TextSize"
        android:layout_alignTop="@+id/imageView5"
        android:layout_toEndOf="@+id/imageView5" />

    <Button
        android:id="@+id/computerSupButton"
        android:layout_width="@dimen/xlargeLandButton_width"
        android:layout_height="@dimen/xlargeLandButton_height"
        android:layout_below="@+id/textView5"
        android:layout_marginStart="@dimen/xlargeLandButton_marginStart"
        android:layout_marginTop="@dimen/xlargeLandButton_marginTop"
        android:layout_toEndOf="@+id/imageView5"
        android:text="@string/TapHereButton"
        android:textSize="18sp" />
</RelativeLayout>
