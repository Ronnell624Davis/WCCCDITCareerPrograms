<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wcccd.wcccditcareerprograms.CisCyberSecurityCert">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="400dp"
        android:layout_height="400dp"
        app:srcCompat="@drawable/logo_wcccd"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="@dimen/xlargeLandTextOverView_width"
        android:layout_height="263dp"
        android:gravity="left"
        android:text="@string/CyberSecurCert_OverView"
        android:textSize="30sp"
        android:layout_alignTop="@+id/imageView7"
        android:layout_toEndOf="@+id/imageView7" />

    <Button
        android:id="@+id/cyberSecurCertButton"
        android:layout_width="@dimen/xlargeLandButton_width"
        android:layout_height="@dimen/xlargeLandButton_height"
        android:layout_marginStart="175dp"
        android:text="@string/TapHereButton"
        android:textSize="18sp"
        android:layout_alignBottom="@+id/imageView7"
        android:layout_toEndOf="@+id/imageView7" />
</RelativeLayout>
