<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wcccd.wcccditcareerprograms.CisComputerSupportSpecialist">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="215dp"
        android:layout_height="215dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        app:srcCompat="@drawable/logo_wcccd" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="310dp"
        android:layout_height="190dp"
        android:layout_below="@+id/imageView5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="11dp"
        android:gravity="left"
        android:text="@string/ComputerSupportSpecialist_Overview"
        android:textSize="17sp" />

    <Button
        android:id="@+id/computerSupButton"
        android:layout_width="130dp"
        android:layout_height="49dp"
        android:layout_marginTop="9dp"
        android:text="@string/TapHereButton"
        android:textSize="11sp"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
