<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wcccd.wcccditcareerprograms.CisComputerSupportSpecialist">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="205dp"
        android:layout_height="205dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/wcccdImgMargin_Top"
        app:srcCompat="@drawable/logo_wcccd" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="@dimen/textOverView_width"
        android:layout_height="163dp"
        android:gravity="left"
        android:text="@string/ComputerSupportSpecialist_Overview"
        android:textSize="@dimen/overviewTextSize"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/imageView5"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/computerSupButton"
        android:layout_width="@dimen/button_width"
        android:layout_height="@dimen/button_height"
        android:text="@string/TapHereButton"
        android:textSize="@dimen/nuttonText_Size"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="22dp" />
</RelativeLayout>
