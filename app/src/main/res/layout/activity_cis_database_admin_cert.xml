<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wcccd.wcccditcareerprograms.CisDatabaseAdminCert">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="205dp"
        android:layout_height="205dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/wcccdImgMargin_Top"
        app:srcCompat="@drawable/logo_wcccd" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="310dp"
        android:layout_height="123dp"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/textOverViewMargin_Top"
        android:gravity="left"
        android:text="@string/DatabaseAdmin_OverView"
        android:textSize="17sp" />

    <Button
        android:id="@+id/dataBaseButton"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:text="@string/TapHereButton"
        android:textSize="12sp"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp" />
</RelativeLayout>
